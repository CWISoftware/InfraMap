@model InfraMap.Web.MVC.Models.AndarModel
@{
    ViewBag.Title = "Terceiro";
}

@Styles.Render("~/bundles/andarCss")
@Styles.Render("~/bundles/jquery-ui")
<h2 class="inline padding-20 margin-20">CWISL - 6 ANDAR</h2>
<section class="andar">
    <div id="partial"></div>
    <div class="fileira">
        @Html.Partial("_Mesa", Model.Mesas, new ViewDataDictionary { { "idMesa", 112 } })
    </div>
</section>

@section Scripts {
    @if (User.IsInRole("GERENTE"))
    {
        @Scripts.Render("~/bundles/jqueryui")
    }
    @Scripts.Render("~/bundles/mesa")
}
